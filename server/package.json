{
    "name": "steadygains-server",
    "version": "1.0.0",
    "description": "Steady Gains Investments - Backend API",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "dev": "node --watch src/index.js",
        "start": "node src/index.js",
        "db:migrate": "npx prisma migrate dev",
        "db:seed": "node src/seed.js",
        "db:studio": "npx prisma studio",
        "db:reset": "npx prisma migrate reset --force && node src/seed.js"
    },
    "dependencies": {
        "@prisma/client": "^6.0.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.0",
        "express": "^5.0.0",
        "jsonwebtoken": "^9.0.0"
    },
    "devDependencies": {
        "prisma": "^6.0.0"
    }
}